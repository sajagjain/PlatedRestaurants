@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_LayoutAboutAndContact.cshtml";
}


<div class="container">
    <div class="jumbotron">
        <h1 class="text-danger">Error.</h1>
        <h3 class="text-danger">An error occurred while processing your request.</h3>
        <h4> @Model.Exception.Message</h4>
        <h4> @Model.ControllerName</h4>
    </div>
</div>


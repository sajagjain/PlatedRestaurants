@model IEnumerable<RestaurantDAL.Enquiry>
@{
    ViewBag.Title = "ShowEnquiries";
    Layout = "~/Views/Shared/_Layout_Restaurant.cshtml";
}


<head>

    <meta name="viewport" content="width=device-width" />


    
</head>

<div class="container" style="margin-top:100px">
    <div class="row">
        <div class="table-responsive">
            <table class="table table-bordered table-hover" style="background-color:white">


                <thead>
                    <tr>
                        <th colspan="3"><h2 align="center">Enquiries</h2></th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th>Enquiry Description</th>
                        <th>Phone No</th>

                        @*<th></th>*@
                        <th>Delete</th>




                    </tr>
                    @foreach (var item in Model)
    {
                <tr>


                    <td>@item.Enquiry_Desc</td>
                    <td>@item.Phone_No</td>


                    @*<td>@Html.ActionLink("Edit", "Edit", "RestaurantOwnerEvent", new { id = item.En_id }, null)</td>*@
                    <td>@Html.ActionLink("Delete", "Delete", "RestaurantOwnerEnquiry", new { id = item.En_id }, null)</td>

                </tr>
}

                    <tr>
                        <td colspan="3" align="center">@Html.ActionLink("Back To Home", "Index", "RestaurantMain")</td>
                    </tr>

</table>
            </div>

        </div>

    </div>





